<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            tools:context=".Level1_qs" >

    <LinearLayout
        android:id="@+id/complete"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:baselineAligned="false" >

        <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100" >

                    <TableLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" >

                        <TableRow
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" >

                            <ImageView
                                    android:id="@+id/symbol_copy"
                                    android:layout_width="0dip"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/opt"
                                    android:layout_weight="0.1"
                                    android:background="@color/colorPrimary"
                                    android:paddingLeft="12dip"/>

                            <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="fill_parent"
                                    android:background="#000000"
                                    android:height="1dip"/>

                            <TextView
                                    android:id="@+id/textView_options"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/options_name"
                                    android:textIsSelectable="false"
                                    android:background="@color/colorPrimary"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:textStyle="bold"
                                    android:padding="12dip" android:autoText="false"/>

                        </TableRow>

                    </TableLayout>

                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#000000"
                            android:height="1dip"/>

                    <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="100" >

                            <TextView
                                    android:id="@+id/text_lower_case_left"
                                    android:layout_width="0dip"
                                    android:layout_height="fill_parent"
                                    android:paddingTop="5dip"
                                    android:paddingBottom="5dip"
                                    android:paddingLeft="10dip"
                                    android:paddingRight="10dip"
                                    android:background="#ffffff"
                                    android:text="@string/abc"
                                    android:textIsSelectable="false"
                                    android:layout_weight="0.3"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"/>

                            <View
                                    android:layout_width="1dip"
                                    android:layout_height="fill_parent"
                                    android:background="#000000"/>

                            <TextView
                                    android:id="@+id/text_lower_case_right"
                                    android:layout_width="0dip"
                                    android:layout_height="50dip"
                                    android:gravity="center"
                                    android:text="@string/lower_case"
                                    android:shadowRadius="2"
                                    android:shadowColor="#dcdcdc"
                                    android:shadowDx="1"
                                    android:shadowDy="1"
                                    android:textColor="#000000"
                                    android:background="#ffffff"
                                    android:paddingTop="5dip"
                                    android:paddingBottom="5dip"
                                    android:paddingLeft="60dip"
                                    android:paddingRight="60dip"
                                    android:layout_weight="1"
                                    android:textAppearance="?android:attr/textAppearanceSmall"/>

                        </LinearLayout>

                    </TableRow>

                </TableLayout>

            </TableRow>

        </TableLayout>

        <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView12"
                android:background="#000000"
                android:height="1dip"/>

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100" >

                    <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="100" >

                            <TextView
                                    android:id="@+id/text_upper_case_left"
                                    android:layout_width="0dip"
                                    android:layout_height="fill_parent"
                                    android:paddingTop="5dip"
                                    android:paddingBottom="5dip"
                                    android:paddingLeft="10dip"
                                    android:paddingRight="10dip"
                                    android:background="#ffffff"
                                    android:text="@string/ABC"
                                    android:layout_weight="0.3"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"/>

                            <View
                                    android:layout_width="1dip"
                                    android:layout_height="fill_parent"
                                    android:background="#000000"/>

                            <TextView
                                    android:id="@+id/text_upper_case_right"
                                    android:layout_width="0dip"
                                    android:layout_height="50dip"
                                    android:gravity="center"
                                    android:text="@string/upper_case"
                                    android:shadowRadius="2"
                                    android:shadowColor="#dcdcdc"
                                    android:shadowDx="1"
                                    android:shadowDy="1"
                                    android:textColor="#000000"
                                    android:background="#ffffff"
                                    android:paddingLeft="60dip"
                                    android:paddingRight="60dip"
                                    android:layout_weight="1"
                                    android:paddingTop="5dip"
                                    android:paddingBottom="5dip"
                                    android:textAppearance="?android:attr/textAppearanceSmall"/>

                        </LinearLayout>

                    </TableRow>

                </TableLayout>

            </TableRow>

        </TableLayout>

        <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#000000"
                android:height="1dip"/>

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100" >

                    <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="100" >

                            <TextView
                                    android:id="@+id/text_number_left"
                                    android:layout_width="0dip"
                                    android:layout_height="fill_parent"
                                    android:paddingTop="5dip"
                                    android:paddingBottom="5dip"
                                    android:paddingLeft="10dip"
                                    android:paddingRight="10dip"
                                    android:background="#ffffff"
                                    android:text="@string/num_l"
                                    android:layout_weight="0.3"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"/>

                            <View
                                    android:layout_width="1dip"
                                    android:layout_height="fill_parent"
                                    android:background="#000000"/>

                            <TextView
                                    android:id="@+id/text_number_right"
                                    android:layout_width="0dip"
                                    android:layout_height="50dip"
                                    android:gravity="center"
                                    android:text="@string/num_r"
                                    android:shadowRadius="2"
                                    android:shadowColor="#dcdcdc"
                                    android:shadowDx="1"
                                    android:shadowDy="1"
                                    android:textColor="#000000"
                                    android:background="#ffffff"
                                    android:paddingLeft="60dip"
                                    android:paddingRight="60dip"
                                    android:layout_weight="1"
                                    android:padding="5dip"
                                    android:paddingTop="5dip"
                                    android:textAppearance="?android:attr/textAppearanceSmall"/>

                        </LinearLayout>

                    </TableRow>

                </TableLayout>

            </TableRow>

        </TableLayout>

        <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#000000"
                android:height="1dip"/>

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100" >

                    <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >

                        <LinearLayout
                                      android:layout_width="fill_parent"
                                      android:layout_height="wrap_content"
                                      android:layout_weight="100" >

                            <TextView
                                    android:id="@+id/text_special_left"
                                    android:layout_width="0dip"
                                    android:layout_height="fill_parent"
                                    android:paddingTop="5dip"
                                    android:paddingBottom="5dip"
                                    android:paddingLeft="10dip"
                                    android:paddingRight="10dip"
                                    android:background="#ffffff"
                                    android:text="@string/special_l"
                                    android:layout_weight="0.3"
                                    android:singleLine="false"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"/>

                            <View
                                    android:layout_width="1dip"
                                    android:layout_height="fill_parent"
                                    android:background="#000000"/>

                            <TextView
                                    android:id="@+id/text_special_right"
                                    android:layout_width="0dip"
                                    android:layout_height="50dip"
                                    android:gravity="center"
                                    android:text="@string/special_r"
                                    android:shadowRadius="2"
                                    android:shadowColor="#dcdcdc"
                                    android:shadowDx="1"
                                    android:shadowDy="1"
                                    android:textColor="#000000"
                                    android:background="#ffffff"
                                    android:paddingLeft="60dip"
                                    android:paddingRight="60dip"
                                    android:layout_weight="1"
                                    android:paddingTop="5dip"
                                    android:padding="5dip" android:textAppearance="?android:attr/textAppearanceSmall"/>

                        </LinearLayout>

                    </TableRow>

                </TableLayout>

            </TableRow>

        </TableLayout>

        <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#000000"
                    android:height="1dip"/>

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100" >

                    <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >

                        <LinearLayout
                                      android:layout_width="fill_parent"
                                      android:layout_height="wrap_content"
                                      android:layout_weight="100" >

                            <TextView
                                    android:id="@+id/text_digits_left"
                                    android:layout_width="0dip"
                                    android:layout_height="fill_parent"
                                    android:paddingTop="5dip"
                                    android:paddingBottom="5dip"
                                    android:paddingLeft="10dip"
                                    android:paddingRight="10dip"
                                    android:background="#ffffff"
                                    android:text="@string/digit_left"
                                    android:layout_weight="0.3"
                                    android:singleLine="false"
                                    android:gravity="center"
                                    android:autoText="true" android:textAppearance="?android:attr/textAppearanceSmall"/>

                            <View
                                    android:layout_width="1dip"
                                    android:layout_height="fill_parent"
                                    android:background="#000000"/>

                            <TextView
                                    android:id="@+id/text_digits_more"
                                    android:layout_width="0dip"
                                    android:layout_height="50dip"
                                    android:gravity="center"
                                    android:text="@string/digit_more"
                                    android:shadowRadius="2"
                                    android:shadowColor="#dcdcdc"
                                    android:shadowDx="1"
                                    android:shadowDy="1"
                                    android:textColor="#000000"
                                    android:background="#ffffff"
                                    android:paddingTop="5dip"
                                    android:paddingBottom="5dip"
                                    android:layout_weight="0.5"
                                    android:textAppearance="?android:attr/textAppearanceSmall"/>

                            <View
                                    android:layout_width="1dip"
                                    android:layout_height="fill_parent"
                                    android:background="#000000"/>

                            <TextView android:id="@+id/text_digits_less"
                                      android:layout_width="0dip"
                                      android:layout_height="50dip"
                                      android:text="@string/digit_less"
                                      android:shadowRadius="2"
                                      android:shadowColor="#dcdcdc"
                                      android:shadowDx="1"
                                      android:shadowDy="1"
                                      android:textColor="#000000"
                                      android:background="#ffffff"
                                      android:paddingTop="5dip"
                                      android:paddingBottom="5dip"
                                      android:layout_weight="0.5"
                                      android:gravity="center"
                                      android:textAppearance="?android:attr/textAppearanceSmall"/>

                        </LinearLayout>

                    </TableRow>

                </TableLayout>

            </TableRow>

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#000000"
                    android:height="1dip"/>

        </TableLayout>

        <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dip"
                        android:layout_weight="100">

                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#ff4444"
                            android:height="1dip" />

                    <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >

                        <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="100" >

                            <TextView
                                    android:id="@+id/text_generate"
                                    android:layout_width="0dip"
                                    android:layout_height="fill_parent"
                                    android:gravity="center"
                                    android:text="@string/generate"
                                    android:shadowRadius="2"
                                    android:shadowColor="#dcdcdc"
                                    android:shadowDx="1"
                                    android:shadowDy="1"
                                    android:textColor="#000000"
                                    android:paddingTop="15dip"
                                    android:paddingBottom="15dip"
                                    android:layout_weight="1"
                                    android:textIsSelectable="false"
                                    android:textStyle="bold"/>

                        </LinearLayout>

                    </TableRow>

                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#ff4444"
                            android:height="1dip"/>

                </TableLayout>

            </TableRow>

        </TableLayout>

        <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dip"
                        android:layout_weight="100" >

                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#000000"
                            android:height="1dip"/>

                    <TableRow
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" >

                        <ImageView
                                android:id="@+id/symbol_password"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:src="@drawable/pw"
                                android:layout_weight="0.1"
                                android:background="@color/colorPrimary"
                                android:paddingLeft="12dip"/>

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="#000000"
                                android:height="1dip"/>

                        <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/password"
                                android:id="@+id/text_password"
                                android:password="false"
                                android:textIsSelectable="false"
                                android:background="@color/colorPrimary"
                                android:layout_weight="1"
                                android:textColor="#000000"
                                android:gravity="center_vertical"
                                android:textStyle="bold"
                                android:padding="12dip"/>

                    </TableRow>

                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#000000"
                            android:height="1dip" />

                    <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >

                        <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="100" >

                            <TextView
                                    android:id="@+id/this_password"
                                    android:layout_width="0dip"
                                    android:layout_height="50dip"
                                    android:gravity="center"
                                    android:text=""
                                    android:shadowRadius="2"
                                    android:shadowColor="#dcdcdc"
                                    android:shadowDx="1"
                                    android:shadowDy="1"
                                    android:textColor="#000000"
                                    android:background="#ffffff"
                                    android:paddingTop="12dip"
                                    android:paddingBottom="12dip"
                                    android:layout_weight="0.8"
                                    android:textIsSelectable="false"
                                    android:textStyle="bold"/>

                            <View
                                    android:layout_width="1dip"
                                    android:layout_height="fill_parent"
                                    android:background="#000000"/>

                            <ImageView
                                    android:id="@+id/symbol_this_copy"
                                    android:layout_width="0dip"
                                    android:layout_height="50dip"
                                    android:src="@drawable/copy"
                                    android:layout_weight="0.2"
                                    android:paddingRight="25dip"
                                    android:paddingLeft="25dip"/>

                        </LinearLayout>

                    </TableRow>

                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#000000"
                            android:height="1dip"/>

                </TableLayout>

            </TableRow>

        </TableLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Developed by Hamedpa"
            android:id="@+id/textView2"
            android:textColor="#000000"
            android:layout_below="@+id/imageView3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    </LinearLayout>

</ScrollView>